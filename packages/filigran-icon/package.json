{
  "name": "filigran-icon",
  "type": "module",
  "version": "0.21.0",
  "description": "Filigran icon",
  "author": "Filigran",
  "license": "Apache-2.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && yarn svgr && tsup",
    "tsup": "tsup",
    "extract-icons": "node script/fetchFromFigma.js FIGMA_FILE_ID=$FIGMA_FILE_ID FIGMA_API_TOKEN=$FIGMA_API_TOKEN",
    "svgr": "svgr --icon --no-dimensions  --title-prop --typescript --jsx-runtime automatic --svg-props role=img -d src --index-template index-template.js  --template svg-template.js -- assets",
    "build:all": "yarn extract-icons && yarn svgr && yarn build"
  },
  "devDependencies": {
    "@svgr/cli": "8.1.0",
    "rimraf": "6.1.2",
    "tsup": "8.5.1",
    "typescript": "5.9.3"
  }
}
