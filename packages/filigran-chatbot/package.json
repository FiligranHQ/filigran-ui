  "version": "1.1.1",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "NODE_ENV=development rollup --watch --config rollup.config.js",
