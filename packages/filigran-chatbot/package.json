    "solid-element": "1.9.1",
    "solid-js": "1.9.11",
    "uuid": "13.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.28.6",
    "@babel/preset-typescript": "7.28.5",
    "@rollup/plugin-babel": "6.1.0",
    "@rollup/plugin-commonjs": "29.0.0",
    "@rollup/plugin-json": "6.1.0",
