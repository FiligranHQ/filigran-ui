{
  "name": "@filigran/ui",
  "description": "Filigran UI library",
  "author": "Filigran",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FiligranHQ/filigran-ui.git"
  },
  "version": "0.44.1",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js"
    },
    "./plugin": {
      "types": "./dist/plugin.d.ts",
      "require": "./dist/plugin.js",
      "import": "./dist/plugin.js"
    },
    "./clients": {
      "types": "./dist/components/clients/index.d.ts",
      "require": "./dist/components/clients/index.js",
      "import": "./dist/components/clients/index.js"
    },
    "./servers": {
      "types": "./dist/components/servers/index.d.ts",
      "require": "./dist/components/servers/index.js",
      "import": "./dist/components/servers/index.js"
    },
    "./auto-form": {
      "types": "./dist/components/auto-form/index.d.ts",
      "require": "./dist/components/auto-form/index.js",
      "import": "./dist/components/auto-form/index.js"
    },
    "./index.css": "./dist/index.css",
    "./theme.css": "./dist/theme.css"
  },
  "scripts": {
    "build": "rimraf dist && tsup",
    "prettier": "prettier . --write"
  },
  "dependencies": {
    "@dnd-kit/core": "6.3.1",
    "@dnd-kit/modifiers": "9.0.0",
    "@dnd-kit/sortable": "10.0.0",
    "@radix-ui/react-accordion": "1.2.12",
    "@radix-ui/react-alert-dialog": "1.1.15",
    "@radix-ui/react-aspect-ratio": "1.1.8",
    "@radix-ui/react-avatar": "1.1.11",
    "@radix-ui/react-checkbox": "1.3.3",
    "@radix-ui/react-dialog": "1.1.15",
    "@radix-ui/react-dropdown-menu": "2.1.16",
    "@radix-ui/react-label": "2.1.8",
    "@radix-ui/react-popover": "1.1.15",
    "@radix-ui/react-radio-group": "1.3.8",
    "@radix-ui/react-select": "2.2.6",
    "@radix-ui/react-separator": "1.1.8",
    "@radix-ui/react-slot": "1.2.4",
    "@radix-ui/react-switch": "1.2.6",
    "@radix-ui/react-tabs": "1.1.13",
    "@radix-ui/react-toast": "1.2.15",
    "@radix-ui/react-toggle": "1.1.10",
    "@radix-ui/react-tooltip": "1.2.8",
    "@tanstack/react-table": "8.21.3",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "cmdk": "1.1.1",
    "date-fns": "4.1.0",
    "embla-carousel-react": "8.6.0",
    "react-color": "2.19.3",
    "react-day-picker": "9.13.0",
    "react-easy-sort": "1.8.0",
    "tailwind-merge": "3.4.0",
    "tailwindcss-animate": "1.0.7"
  },
  "devDependencies": {
    "@types/react-color": "3.0.13",
    "@types/react-dom": "19.2.3",
    "autoprefixer": "10.4.23",
    "postcss": "8.5.6",
    "prettier": "3.7.4",
    "prettier-plugin-organize-imports": "4.3.0",
    "prettier-plugin-tailwindcss": "0.7.2",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "rimraf": "6.1.2",
    "tailwindcss": "3.4.18",
    "tailwindcss-scoped-preflight": "3.5.4",
    "terser": "5.44.1",
    "tsup": "8.5.1",
    "typescript": "5.9.3"
  },
  "peerDependencies": {
    "@filigran/icon": "^0.22.3",
    "@hookform/resolvers": "^5.0.0",
    "@types/react": "^16.8 || ^17.0 || ^18.0 || ^19.0",
    "react": "^16.8 || ^17.0 || ^18.0 || ^19.0",
    "react-dom": "^16.8 || ^17.0 || ^18.0 || ^19.0",
    "react-hook-form": "^7.68.0",
    "zod": "^4.1.13"
  },
  "prettier": {
    "printWidth": 80,
    "semi": false,
    "bracketSameLine": true,
    "trailingComma": "es5",
    "tabWidth": 2,
    "singleQuote": true,
    "bracketSpacing": false,
    "arrowParens": "always",
    "singleAttributePerLine": true,
    "plugins": [
      "prettier-plugin-tailwindcss",
      "prettier-plugin-organize-imports"
    ]
  }
}
