{
  "name": "filigran-ui",
  "version": "0.42.0",
  "description": "Filigran UI library",
  "author": "Filigran",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FiligranHQ/filigran-ui.git"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js"
    },
    "./plugin": {
      "types": "./dist/plugin.d.ts",
      "require": "./dist/plugin.js",
      "import": "./dist/plugin.js"
    },
    "./clients": {
      "types": "./dist/components/clients/index.d.ts",
      "require": "./dist/components/clients/index.js",
      "import": "./dist/components/clients/index.js"
    },
    "./servers": {
      "types": "./dist/components/servers/index.d.ts",
      "require": "./dist/components/servers/index.js",
      "import": "./dist/components/servers/index.js"
    },
    "./auto-form": {
      "types": "./dist/components/auto-form/index.d.ts",
      "require": "./dist/components/auto-form/index.js",
      "import": "./dist/components/auto-form/index.js"
    },
    "./index.css": "./dist/index.css",
    "./theme.css": "./dist/theme.css"
  },
  "scripts": {
    "build": "rimraf dist && tsup",
    "prettier": "prettier . --write"
  },
  "dependencies": {
    "@dnd-kit/core": "6.1.0",
    "@dnd-kit/modifiers": "7.0.0",
    "@dnd-kit/sortable": "8.0.0",
    "@hookform/resolvers": "5.2.2",
    "@radix-ui/react-accordion": "1.2.11",
    "@radix-ui/react-alert-dialog": "1.1.14",
    "@radix-ui/react-aspect-ratio": "1.1.7",
    "@radix-ui/react-avatar": "1.1.10",
    "@radix-ui/react-checkbox": "1.3.2",
    "@radix-ui/react-dialog": "1.1.14",
    "@radix-ui/react-dropdown-menu": "2.1.15",
    "@radix-ui/react-label": "2.1.7",
    "@radix-ui/react-popover": "1.1.14",
    "@radix-ui/react-radio-group": "1.3.7",
    "@radix-ui/react-select": "2.2.5",
    "@radix-ui/react-separator": "1.1.7",
    "@radix-ui/react-slot": "1.2.3",
    "@radix-ui/react-switch": "1.2.5",
    "@radix-ui/react-tabs": "1.1.12",
    "@radix-ui/react-toast": "1.2.14",
    "@radix-ui/react-toggle": "1.1.9",
    "@radix-ui/react-tooltip": "1.2.7",
    "@tanstack/react-table": "8.21.2",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "cmdk": "1.1.1",
    "date-fns": "4.1.0",
    "embla-carousel-react": "8.6.0",
    "react-color": "2.19.3",
    "react-day-picker": "8.10.1",
    "react-easy-sort": "1.6.0",
    "react-hook-form": "7.55.0",
    "tailwind-merge": "2.3.0",
    "tailwindcss-animate": "1.0.7",
    "zod": "4.1.13"
  },
  "devDependencies": {
    "@types/react-color": "3.0.13",
    "@types/react-dom": "18.3.7",
    "autoprefixer": "10.4.21",
    "postcss": "8.5.6",
    "prettier": "3.6.2",
    "prettier-plugin-organize-imports": "4.1.0",
    "prettier-plugin-tailwindcss": "0.6.13",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "rimraf": "6.1.2",
    "tailwindcss": "3.4.4",
    "tailwindcss-scoped-preflight": "3.4.12",
    "terser": "5.43.1",
    "tsup": "8.5.0",
    "typescript": "5.8.3"
  },
  "peerDependencies": {
    "@types/react": "^16.8 || ^17.0 || ^18.0 || ^19.0",
    "filigran-icon": "^0.21.0",
    "react": "^16.8 || ^17.0 || ^18.0 || ^19.0",
    "react-dom": "^16.8 || ^17.0 || ^18.0 || ^19.0"
  },
  "prettier": {
    "printWidth": 80,
    "semi": false,
    "bracketSameLine": true,
    "trailingComma": "es5",
    "tabWidth": 2,
    "singleQuote": true,
    "bracketSpacing": false,
    "arrowParens": "always",
    "singleAttributePerLine": true,
    "plugins": [
      "prettier-plugin-tailwindcss",
      "prettier-plugin-organize-imports"
    ]
  }
}
